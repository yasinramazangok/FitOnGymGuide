@using EntityLayer.Concretes
@model List<Blog>

@*
    Partial View
*@

@foreach (var item in Model)
{
    <div class="post-content-body">
        <p>@item.Content</p>
        <div class="row mb-5 mt-5">
            <div class="col-md-12 mb-4">
                <img src="@item.ImageUrl" alt="Image placeholder" class="img-fluid rounded">
            </div>
        </div>  
    </div>
}
