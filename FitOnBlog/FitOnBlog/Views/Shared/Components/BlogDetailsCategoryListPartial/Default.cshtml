@using EntityLayer.Concretes
@model List<Category>

@*
    Partial View
*@

<div class="sidebar-box" style="background:transparent">
    <h3 class="heading">Kategoriler</h3>
    <ul class="categories">
        @foreach (var item in Model)
        {
            <li><a href="#">@item.Name<span></span></a></li>
        }
    </ul>
</div>